{
  "name": "@storipress/apollo-vue-devtool",
  "version": "0.0.1",
  "main": "dist/apollo-vue-devtool.cjs.js",
  "module": "dist/apollo-vue-devtool.esm-bundler.js",
  "types": "dist/index.d.ts",
  "workspaces": [
    ".",
    "example"
  ],
  "exports": {
    ".": {
      "require": "./dist/apollo-vue-devtool.cjs.js",
      "browser": "./dist/apollo-vue-devtool.esm-browser.js",
      "import": "./dist/apollo-vue-devtool.esm-bundler.js",
      "module": "./dist/apollo-vue-devtool.esm-bundler.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "dev": "tsc --watch -d",
    "build:esbuild": "rimraf dist && tsc -d --emitDeclarationOnly && node ./esbuild.js",
    "build:rollup": "rimraf dist && rollup -c rollup.config.js"
  },
  "dependencies": {
    "@vue/devtools-api": "^6.0.12",
    "zen-observable-ts": "^1.2.3"
  },
  "peerDependencies": {
    "@apollo/client": "^3.0.0",
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@apollo/client": "^3.5.10",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/node": "^17.0.21",
    "@vue/compiler-sfc": "^3.2.31",
    "esbuild": "^0.14.23",
    "esbuild-plugin-vue": "^0.2.4",
    "pascalcase": "^1.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.68.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "rollup-plugin-vue": "^6.0.0",
    "typescript": "^4.5.5",
    "vue": "^3.2.31"
  },
  "packageManager": "yarn@3.1.1"
}
